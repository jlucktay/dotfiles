set atblanks
set autoindent
set boldtext
set constantshow
set fill 72
set linenumbers
set smarthome
set softwrap
set tabsize 2
set trimblanks

unset casesensitive
unset nonewlines
unset nowrap

bind M-B prevword all
bind M-F nextword all

include /usr/local/var/homebrew/linked/nanorc/share/nanorc/*.nanorc
